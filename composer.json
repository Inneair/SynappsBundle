{
	"name" : "inneair/synapps-bundle",
	"type" : "symfony-bundle",
	"description" : "Synapps bundle for Symfony",
	"keywords" : [
		"inneair",
		"synapps",
		"file",
		"system",
		"service",
		"controller",
		"aop",
		"transaction"
	],
	"license" : "MIT",
	"authors" : [{
			"name" : "InnÃ©air",
			"homepage" : "http://www.inneair.com/"
		}
	],
	"repositories" : [{
			"type" : "git",
			"url" : "https://github.com/inneair/synapps"
		}
