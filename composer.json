{
	"name" : "inneair/synapps-bundle",
	"type" : "symfony-bundle",
	"description" : "Synapps bundle for Symfony",
	"keywords" : [
		"inneair",
		"synapps",
		"file",
		"system",
		"service",
		"controller",
		"aop",
		"transaction"
	],
	"license" : "MIT",
	"authors" : [{
			"name" : "InnÃ©air",
			"homepage" : "http://www.inneair.com/"
		}
	],
	"repositories" : [{
			"type" : "git",
			"url" : "https://github.com/inneair/synapps"
		}
	],
	"autoload" : {
		"psr-4" : {
			"Inneair\\SynappsBundle\\" : "src/",
			"Inneair\\SynappsBundle\\Test\\" : "tests/"
		}
	},
	"require" : {
		"php" : ">=5.6",
		"inneair/synapps" : "^1.0",
		"inneair/transaction-bundle": "^1.0",
		"symfony/symfony" : "3.2.*",
		"sensio/framework-extra-bundle": "^3.0.2",
		"doctrine/orm" : "^2.5",
		"friendsofsymfony/rest-bundle" : "^2.1",
		"psr/log" : "~1.0",
		"jms/aop-bundle" : "^1.2",
		"jms/serializer-bundle" : "^1.1",
		"jms/serializer" : "^1.4"
	},
	"require-dev" : {
		"phpunit/phpunit" : "^5.7"
	},
	"minimum-stability" : "stable",
	"config" : {
		"bin-dir" : "bin"
	},
	"extra" : {
		"branch-alias" : {
			"dev-master" : "1.1.x-dev"
		}
	}
}