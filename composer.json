	"autoload" : {
		"psr-4" : {
			"Inneair\\SynappsBundle\\" : "src/"
		}
	},
	"require" : {
		"php" : ">=5.6",
		"inneair/synapps" : "~1.0-dev",
		"symfony/symfony" : "~3.0.0",
		"sensio/framework-extra-bundle" : "~3.0",
		"doctrine/orm" : "~2.2,>=2.2.3",
		"friendsofsymfony/rest-bundle" : "~1.2",
		"psr/log" : "~1.0",
		"jms/aop-bundle" : "~1.0",
		"jms/serializer-bundle" : "~1.1",
		"jms/serializer" : "~1.1"
	},
	"require-dev" : {
		"phpunit/phpunit" : "~5.0"
	},
	"minimum-stability" : "stable",
	"config" : {
		"bin-dir" : "bin"
	},
	"extra" : {
		"branch-alias" : {
			"dev-master" : "1.1.x-dev"
		}
	}
}